<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//ENhttp://www.apple.com/DTDs/PropertyList-1.0.dtd">
    <plist version="1.0">
        <dict>
            <key>Program</key>
                <string>{{fixity_path}}</string>
            <key>Label</key>
            <string>{{demon_name}}</string>
            <key>ProgramArguments</key>
            <array>
                <string>{{fixity_path}}</string>
                <string>-a={{project_name}}</string>
            </array>
            <key>StandardOutPath</key>
            <string>{{debug_file_path}}</string>
            <key>StandardErrorPath</key>
            <string>{{debug_file_path}}</string>
            <key>StartCalendarInterval</key>
            <dict>
            <key>Minute</key>
            <integer>{{mins}}</integer>
            <key>Hour</key>
            <integer>{{hrs}}</integer>
            </dict>
        </dict>
    </plist>
